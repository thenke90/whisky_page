<%include partials/header %>
<div class="container">
    <header class="jumbotron">
         <div>
               <h1>Tasted Whiskys</h1> 
               <p> View all Whiskies I have ever tasted</p>
               <p>
                   <a class="btn btn-primary btn-lg" href="https://whiskyhomepage-up1990.c9users.io/">click to home</a>    
               </p>
         </div>  
    </header>
    
    <div class="row">
        <div class="col-lg-12">
            <h3>
                All Whiskies
            </h3>
        </div>
     </div>    
        
     
    <div class="row text-center" style="display:flex;flex-wrap: wrap;">
             <%whisky.forEach(function(whisky){%>
                    <div class="col-md-3 col-sm-6">
                        <div class="caption">
                             <h1><%=whisky.distillery%></h1>
                        </div>     
                        <div class="thumbnail">     
                            <img src="<%=whisky.image%>">
                        </div>
                    </div>
                <% }); %>
    </div>     
</div>    
    <%include partials/footer %>